 {{#each dataObjectMappings}}
update omd.PARAMETER set PARAMETER_VALUE_CODE='with cte as (

select  * from {{sourceDataObjects.0.name}} as source

where source.OMD_CURRENT_RECORD_INDICATOR =''Y'' and source.OMD_DELETED_RECORD_INDICATOR=''N''

and sha1(UPPER(CAST({{#each businessKeys}}{{#each businessKeyComponentMapping}} RTRIM(IFNULL(source.{{sourceDataItems.0.name}},''NA'')){{#unless @last}}||''|''||{{/unless}}{{/each}} {{/each}} AS STRING))) not in

(select sha1(UPPER(CAST({{#each businessKeys}}{{#each businessKeyComponentMapping}} RTRIM(IFNULL(target.{{sourceDataItems.0.name}},''NA'')){{#unless @last}}||''|''||{{/unless}}{{/each}} {{/each}} AS STRING))) from DL_{{targetDataObject.name}} AS target)

)

INSERT INTO {{sourceDataObjects.0.name}} (
{{#each dataItemMappings}}
  {{sourceDataItems.0.name}}{{#unless @last}},{{/unless}}
   {{/each}}
,
{{../metadataConfiguration.recordSourceAttribute}},
OMD_FILE_LOCATION,
{{../metadataConfiguration.eventDateTimeAttribute}},
OMD_HASH_DIFF,
OMD_CURRENT_RECORD_INDICATOR,
OMD_DELETED_RECORD_INDICATOR,
OMD_EXPIRY_DATETIME,
OMD_UPDATE_MODULE_INSTANCE_ID,
[{{../metadataConfiguration.loadDateTimeAttribute}}],
{{../metadataConfiguration.etlProcessAttribute}}
)

SELECT  {{#each dataItemMappings}}
  delete_insert.{{sourceDataItems.0.name}}{{#unless @last}},{{/unless}}
   {{/each}}
,delete_insert.{{../metadataConfiguration.recordSourceAttribute}},
delete_insert.OMD_FILE_LOCATION,
CURRENT_TIMESTAMP() AS {{../metadataConfiguration.eventDateTimeAttribute}},
delete_insert.OMD_HASH_DIFF,
''Y'',
''Y'',
''9999-12-31'' 
, ''NULL''
,' where parameter_key_code='DL_SILVER_DELETE_{{sourceDataObjects.0.name}}';

{{/each}}
