--
-- DIRECT registration template for the Link object {{targetDataObject.name}}.
--

DECLARE @SOURCE_TABLE VARCHAR(1000);
DECLARE @TARGET_TABLE VARCHAR(1000);
DECLARE @SYSTEM_GROUP VARCHAR(100);

{{#each dataObjectMappings}}

SET @SOURCE_TABLE = '{{sourceDataObjects.0.name}}';
SET @TARGET_TABLE = '{{targetDataObject.name}}';

   SELECT @SYSTEM_GROUP = SUBSTRING('{{sourceDataObjects.0.name}}',charindex('_', '{{sourceDataObjects.0.name}}', 1)+1, charindex('_', '{{sourceDataObjects.0.name}}', (charindex('_', '{{sourceDataObjects.0.name}}', 1))+1) -charindex('_', '{{sourceDataObjects.0.name}}', 1)-1 )

/*
   Add Module into OMD.MODULE table
*/

   INSERT INTO [omd].[MODULE]
           ([MODULE_CODE]
           ,[MODULE_DESCRIPTION]
           ,[MODULE_TYPE]
           ,[DATA_OBJECT_SOURCE]
           ,[DATA_OBJECT_TARGET]
           ,[AREA_CODE]
           ,[FREQUENCY_CODE]
           ,[INACTIVE_INDICATOR])
     
	 SELECT DISTINCT 'm_200_'+@TARGET_TABLE+'_'+@SOURCE_TABLE,@TARGET_TABLE+'_'+@SOURCE_TABLE+' data processing from STG','INT','STG',
	 'INT','INT','Continuous','N'
	 from OMD.MODULE dm
	 left outer join omd.module m on m.MODULE_CODE='m_200_'+@TARGET_TABLE+'_'+@SOURCE_TABLE
	 where dm.MODULE_ID = 0
		and m.MODULE_ID is null

/*
   Add Batch into OMD.BATCH table
*/


   INSERT INTO [omd].[BATCH]
           ([BATCH_CODE]
           ,[BATCH_DESCRIPTION]
           ,[FREQUENCY_CODE]
           ,[INACTIVE_INDICATOR]
           ,[NEXT_RUN_INDICATOR])

	 SELECT DISTINCT 'b_INT_'+@TARGET_TABLE,@TARGET_TABLE+' data processing into INT layer ','Continuous','N',NULL
	 from OMD.MODULE dm
	 left outer join omd.batch b on b.batch_code='b_INT_'+@TARGET_TABLE
	 where dm.MODULE_ID = 0
		and b.BATCH_ID is null


	 INSERT INTO [omd].[BATCH]
           ([BATCH_CODE]
           ,[BATCH_DESCRIPTION]
           ,[FREQUENCY_CODE]
           ,[INACTIVE_INDICATOR]
           ,[NEXT_RUN_INDICATOR])

	 SELECT DISTINCT 'b_INT_'+@TARGET_TABLE+'_'+@SOURCE_TABLE,@TARGET_TABLE+' data processing from '+@SOURCE_TABLE,'Continuous','N',NULL
	 from OMD.MODULE dm
	 left outer join omd.batch b on b.batch_code='b_INT_'+@TARGET_TABLE+'_'+@SOURCE_TABLE
	 where dm.MODULE_ID = 0
		and b.BATCH_ID is null

/*
   Add Batch-Module relationship into OMD.BATCH_MODULE table
*/


	 INSERT INTO [omd].[BATCH_MODULE]
           ([BATCH_ID]
           ,[MODULE_ID]
           ,[INACTIVE_INDICATOR])

   SELECT DISTINCT b.batch_id,m.module_id,'N'
   from OMD.MODULE dm
   left outer join omd.batch b on b.batch_code='b_INT_'+@TARGET_TABLE+'_'+@SOURCE_TABLE
   left outer join omd.module m on m.MODULE_CODE='m_200_'+@TARGET_TABLE+'_'+@SOURCE_TABLE
   left outer join omd.batch_module bm on b.BATCH_ID=bm.BATCH_ID and bm.MODULE_ID=m.MODULE_ID
   where dm.MODULE_ID = 0
		and bm.BATCH_ID is null



/*
   Add Batch-Batch relationship into OMD.BATCH_BATCH table
*/


   INSERT INTO [omd].[BATCH_BATCH]
           ([PARENT_BATCH_ID]
           ,[BATCH_ID]
           ,[SYSTEM_GROUP]
           ,[SEQUENCE]
           ,[BATCH_SEQUENCE]
           ,[NEXT_RUN_INDICATOR])

   SELECT DISTINCT pb.BATCH_ID as PARENT_BATCH_ID,
         cb.BATCH_ID as BATCH_ID,
         @SYSTEM_GROUP as SYSTEM_GROUP,
         bb.MAX_SEQUENCE + 1 as [SEQUENCE],
         'Sequence' as BATCH_SEQUENCE,
         NULL as NEXT_RUN_INDICATOR

   from OMD.MODULE dm
   left outer join OMD.BATCH pb ON pb.BATCH_CODE = 'b_INT_'+@TARGET_TABLE
   left outer join OMD.BATCH cb ON cb.BATCH_CODE = 'b_INT_'+@TARGET_TABLE+'_'+@SOURCE_TABLE
   left outer join OMD.BATCH_BATCH bb2 ON cb.BATCH_ID = bb2.BATCH_ID
   left outer join (SELECT DISTINCT b.BATCH_ID,
                  COALESCE(MAX(bb.[SEQUENCE]),0) as MAX_SEQUENCE
               from OMD.BATCH b
               left outer join OMD.BATCH_BATCH bb ON b.BATCH_ID = bb.PARENT_BATCH_ID
               WHERE b.BATCH_CODE = 'b_INT_'+@TARGET_TABLE
               GROUP BY b.BATCH_ID) as bb ON bb.BATCH_ID = pb.BATCH_ID
   WHERE dm.MODULE_ID = 0
         and bb2.BATCH_BATCH_ID is null


   INSERT INTO [omd].[BATCH_BATCH]
           ([PARENT_BATCH_ID]
           ,[BATCH_ID]
           ,[SYSTEM_GROUP]
           ,[SEQUENCE]
           ,[BATCH_SEQUENCE]
           ,[NEXT_RUN_INDICATOR])

   SELECT DISTINCT pb.BATCH_ID as PARENT_BATCH_ID,
         cb.BATCH_ID as BATCH_ID,
         @SYSTEM_GROUP as SYSTEM_GROUP,
         1 as [SEQUENCE],
         'Concurrent' as BATCH_SEQUENCE,
         NULL as NEXT_RUN_INDICATOR

   from OMD.MODULE dm
   left outer join OMD.BATCH pb ON pb.BATCH_CODE = 'b_INT_LNK'
   left outer join OMD.BATCH cb ON cb.BATCH_CODE = 'b_INT_'+@TARGET_TABLE
   left outer join OMD.BATCH_BATCH bb ON cb.BATCH_ID = bb.BATCH_ID

   WHERE dm.MODULE_ID = 0
         and bb.BATCH_ID is null

/*
   Add pattern to Module into OMD.MODULE_PATTERN table
*/


   INSERT INTO [omd].[MODULE_PATTERN]
         ([MODULE_ID]
         ,[PATTERN_ID]
         ,[INACTIVE_INDICATOR])

   SELECT DISTINCT m.module_id, 6,'N' from omd.module m
   left outer join omd.module_pattern mp on m.module_id=mp.module_id
   where  m.MODULE_CODE='m_200_'+@TARGET_TABLE+'_'+@SOURCE_TABLE
      and mp.MODULE_ID is null



